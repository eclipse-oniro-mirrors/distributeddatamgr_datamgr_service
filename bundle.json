            ],
            "third_party": [
                "cJSON",
                "jsoncpp",
                "libuv",
                "openssl",
                "sqlite",
                "zlib"
            ]
        },
        "build": {
            "sub_component": [
                "//foundation/distributeddatamgr/datamgr_service/services/distributeddataservice/app:build_module",
                "//foundation/distributeddatamgr/datamgr_service/services/distributeddataservice/framework:build_module",
